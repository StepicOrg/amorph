#!/usr/bin/env python3
import argparse

from ddfeedback import get_description_of_changes


def main():
    parser = argparse.ArgumentParser()
    parser.add_argument('left_code', type=str)
    parser.add_argument('right_code', type=str)
    args = parser.parse_args()
    print('\n'.join(get_description_of_changes(args.left_code, args.right_code)))


if __name__ == '__main__':
    main()
